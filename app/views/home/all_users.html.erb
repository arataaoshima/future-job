<h1 class="text-center top-message-wrapper">Users List</h1>

<div class="container">
  <table class="table table-dark">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">useranme</th>
        <th scope="col">email</th>
        <th scope="col">admin</th>
        <th scope="col">paid?</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <th scope="row"><%= link_to user.id, "/edit_user/#{user.id}" %></th>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.admin %></td>
        <td><%= user.payment %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
