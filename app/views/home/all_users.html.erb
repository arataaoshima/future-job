<h1>Users List</h1>


<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">useranme</th>
      <th scope="col">email</th>
      <th scope="col">admin</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <th scope="row"><%= link_to user.id, "/edit_user/#{user.id}" %></th>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.admin %></td>
    </tr>
    <% end %>
  </tbody>
</table>
